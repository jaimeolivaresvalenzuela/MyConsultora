<p id="notice"><%= notice %></p>

<h1>Listing Proyectos</h1>

<table class="table">
  <thead>
    <tr>
      <th>ROL PERSONA</th>
      <th>CUENTA</th>
      <th>NOMBRE DEL PROYECTO</th>
      <th>ESTADO DEL PROYECTO</th>
      <th>FECHA DE CREACION</th>
      <th>DESCRIPCION DEL PROYECTO</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyectos.each do |proyecto| %>
      <tr>
        <td><%= proyecto.ropeid %></td>
        <td><%= proyecto.cueid %></td>
        <td><%= proyecto.pronombre %></td>
        <td><%= proyecto.proestado %></td>
        <td><%= proyecto.profechacreacion %></td>
        <td><%= proyecto.prodescripcion %></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Show', proyecto %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Edit', edit_proyecto_path(proyecto) %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button type="button" class="btn btn-default"><%= link_to 'New Proyecto', new_proyecto_path %></button>
