<p id="notice"><%= notice %></p>

<h1>Listing Etapatareas</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Tarea</th>
      <th>Rol Persona</th>
      <th>Estado</th>
      <th>Fecha de Creacion</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @etapatareas.each do |etapatarea| %>
      <tr>
        <td><%= etapatarea.pretid %></td>
        <td><%= etapatarea.tarid %></td>
        <td><%= etapatarea.ropeid %></td>
        <td><%= etapatarea.ettaestado %></td>
        <td><%= etapatarea.ettafechacreacion %></td>
        <td><%= etapatarea.ettadescripcion %></td>
        <td><%= link_to 'Show', etapatarea %></td>
        <td><%= link_to 'Edit', edit_etapatarea_path(etapatarea) %></td>
        <td><%= link_to 'Destroy', etapatarea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Etapatarea', new_etapatarea_path %>
