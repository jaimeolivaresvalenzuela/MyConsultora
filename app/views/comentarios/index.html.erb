<p id="notice"><%= notice %></p>

<h1>Listing Comentarios</h1>

<table class="table">
  <thead>
    <tr>
      <th>TAREA DEL PROYECTO </th>
      <th>PROYECTO</th>
      <th>ETAPA DEL PROYECTO</th>
      <th>COMENTARIO</th>
      <th>ESTADO</th>
      <th>FECHA DE CREACION</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comentarios.each do |comentario| %>
      <tr>
        <td><%= comentario.ettaid %></td>
        <td><%= comentario.proid %></td>
        <td><%= comentario.pretid %></td>
        <td><%= comentario.comcomentario %></td>
        <td><%= comentario.comestado %></td>
        <td><%= comentario.comfechacreacion %></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Show', comentario %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Edit', edit_comentario_path(comentario) %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Destroy', comentario, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button type="button" class="btn btn-default"><%= link_to 'New Comentario', new_comentario_path %></button>
