<p id="notice"><%= notice %></p>

<h1>Listing Etapas</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre Etapa</th>
      <th>Estado Etapa</th>
      <th>Fecha De Creacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @etapas.each do |etapa| %>
      <tr>
        <td><%= etapa.etanombre %></td>
        <td><%= etapa.etaestado %></td>
        <td><%= etapa.etafechacreacion %></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Show', etapa %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Edit', edit_etapa_path(etapa) %></button></td>
        <td><button type="button" class="btn btn-default"><%= link_to 'Destroy', etapa, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button type="button" class="btn btn-default"><%= link_to 'New Etapa', new_etapa_path %></button>
