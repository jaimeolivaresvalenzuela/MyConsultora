<p id="notice"><%= notice %></p>

<h1>Listing Contactos</h1>

<table>
  <thead>
    <tr>
      <th>Cliid</th>
      <th>Connombre</th>
      <th>Conapellidopaterno</th>
      <th>Conapellidomaterno</th>
      <th>Concargo</th>
      <th>Contelefono</th>
      <th>Conmail</th>
      <th>Conestado</th>
      <th>Confechacreacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactos.each do |contacto| %>
      <tr>
        <td><%= contacto.cliid %></td>
        <td><%= contacto.connombre %></td>
        <td><%= contacto.conapellidopaterno %></td>
        <td><%= contacto.conapellidomaterno %></td>
        <td><%= contacto.concargo %></td>
        <td><%= contacto.contelefono %></td>
        <td><%= contacto.conmail %></td>
        <td><%= contacto.conestado %></td>
        <td><%= contacto.confechacreacion %></td>
        <td><%= link_to 'Show', contacto %></td>
        <td><%= link_to 'Edit', edit_contacto_path(contacto) %></td>
        <td><%= link_to 'Destroy', contacto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contacto', new_contacto_path %>
