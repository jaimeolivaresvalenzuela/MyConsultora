<p id="notice"><%= notice %></p>

<h1>Listing Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Razon Social</th>
      <th>Direccion</th>
      <th>Rut</th>
      <th>Digito</th>
      <th>Estado</th>
      <th>Fecha Creacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.clinombre %></td>
        <td><%= cliente.clirazonsocial %></td>
        <td><%= cliente.clidireccion %></td>
        <td><%= cliente.clirut %></td>
        <td><%= cliente.clidv %></td>
        <td><%= cliente.cliestado %></td>
        <td><%= cliente.clifechacreacion %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
