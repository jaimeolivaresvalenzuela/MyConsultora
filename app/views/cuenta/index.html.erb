<p id="notice"><%= notice %></p>

<h1>Listing Cuenta</h1>

<table>
  <thead>
    <tr>
      <th>Rol Persona</th>
      <th>Cliente</th>
      <th>Nombre</th>
      <th>Estado</th>
      <th>Fecha Creacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuenta.each do |cuentum| %>
      <tr>
        <td><%= cuentum.ropeid %></td>
        <td><%= cuentum.cliid %></td>
        <td><%= cuentum.cuenombre %></td>
        <td><%= cuentum.cueestado %></td>
        <td><%= cuentum.cuefechacreacion %></td>
        <td><%= link_to 'Show', cuentum %></td>
        <td><%= link_to 'Edit', edit_cuentum_path(cuentum) %></td>
        <td><%= link_to 'Destroy', cuentum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cuentum', new_cuentum_path %>
